int	ft_in_q(int in_q)
{
	if (in_q == 0)
		return (1);
	else
		return (0);
}#include "./minishell.h"



void	ft_unlink_hd(t_exec *begin)
{
	t_exec *tmp;

	tmp = begin;
	while (tmp)
	{
		if (begin->hd_filename != NULL)
		{
			unlink(begin->hd_filename);
		}
		tmp = tmp->next;
	}
	
}
